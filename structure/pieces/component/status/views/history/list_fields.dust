<table id="table_e_history" data-url="/history/datalist" class="dataTable table table-bordered table-striped">
    <thead class="main">
        <tr class="fields">
            <th data-field="id" data-col="id">
                {@__ key="entity.e_history.id_entity"/}&nbsp;
                {@__ key="entity.e_history.label_entity"/}
            </th>
            <th data-field="r_status.id" data-col="r_status.id" data-type="string">
                {@__ key="entity.e_history.r_status"/}&nbsp;-&nbsp;
                {@__ key="entity.e_status.id_entity"/}
            </th>
            <th data-field="f_comment" data-col="f_comment" data-type="text">
                {@__ key="entity.e_history.f_comment"/}
            </th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <thead class="filters">
        <tr class="fields">
            <th data-field="id" data-col="id">
                {@__ key="entity.e_history.id_entity"/}&nbsp;
                {@__ key="entity.e_history.label_entity"/}
            </th>
            <th data-field="r_status.id" data-col="r_status.id" data-type="string">
                {@__ key="entity.e_history.r_status"/}&nbsp;-&nbsp;
                {@__ key="entity.e_status.id_entity"/}
            </th>
            <th data-field="f_comment" data-col="f_comment" data-type="text">
                {@__ key="entity.e_history.f_comment"/}
            </th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {#e_history}
        <tr id="bodyTR">
            <td data-field="id">{id}</td>
            <td data-field="r_status.id" data-type="string">{r_status.id}</td>
            <td data-field="f_comment" data-type="text">{f_comment|s}</td>
            <td>
                {@eq key=for value="fieldset"}
                <form class="fieldsetform">
                    <input type="hidden" value="{id}" name="idRemove">
                    <button type="submit" class="btn btn-danger"><i class="fa fa-times fa-md">&nbsp;&nbsp;</i><span>{@__ key="button.remove" /}</span></button>
                </form>
                {/eq}
                {@eq key=for value="hasMany"}
                <!--<a class="btn btn-primary" href="/history/show?id={id}&amp;hideButton=1"><i class="fa fa-plus fa-md">&nbsp;&nbsp;</i><span>{@__ key="button.show" /}-->
                {/eq}
            </td>
            <td>
                {@eq key=for value="hasMany"}
                <a class="btn btn-warning" href="/history/update_form?id={id}&amp;associationAlias={associationAlias}&amp;associationForeignKey={associationForeignKey}&amp;associationFlag={associationFlag}&amp;associationSource={associationSource}&amp;associationUrl={associationUrl}"><i class="fa fa-pencil fa-md">&nbsp;&nbsp;</i><span>{@__ key="button.update" /}</span></a>
                {/eq}
            </td>
            <td>
                {@eq key=for value="hasMany"}
                <form action="/history/delete" method="post">
                    <button onclick="return confirm('Etes-vous sÃ»r de vouloir supprimer cet enregistrement ?');" class="btn btn-danger"><i class="fa fa-trash-o fa-md">&nbsp;&nbsp;</i>
			                <span>{@__ key="button.delete" /}</span>
			                <input name="id" value="{id}" type="hidden">
			                <input name="associationAlias" value="{associationAlias}" type="hidden">
			                <input name="associationForeignKey" value="{associationForeignKey}" type="hidden">
			                <input name="associationFlag" value="{associationFlag}" type="hidden">
			                <input name="associationSource" value="{associationSource}" type="hidden">
			                <input name="associationUrl" value="{associationUrl}" type="hidden">
			            </button>
                </form>
                {/eq}
            </td>
        </tr>
        {/e_history}
    </tbody>
</table>